{
  "name": "vite-project",
  "homepage": "https://aspasht.github.io/vite-project",
  "private": true,
  "version": "0.0.0",
  "type": "module",
 "scripts": {
  "dev": "vite",
  "docs:dev": "vitepress dev .vitepress",
  "build": "tsc && vite build",
  "docs:build": "vitepress build .vitepress",
  "move-blog": "rm -rf dist/blog && mkdir -p dist && cp -r .vitepress/dist dist/blog",
  "build:all": "npm run build && npm run docs:build && npm run move-blog",
  "predeploy": "npm run build:all",
  "deploy": "gh-pages -d dist"
},

  "dependencies": {
    "@headlessui/react": "^1.7.19",
    "axios": "^1.4.0",
    "connect": "^3.7.0",
    "gsap": "^3.12.5",
    "http-proxy-middleware": "^2.0.6",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-intersection-observer": "^9.4.3",
    "react-markdown": "^8.0.7",
    "react-router-dom": "^6.10.0",
    "react-spring": "^9.7.1",
    "react-syntax-highlighter": "^15.5.0",
    "remark-gfm": "^3.0.1",
    "typed.js": "^2.0.16"
  },
  "devDependencies": {
    "@tailwindcss/typography": "^0.5.9",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.0.11",
    "@typescript-eslint/eslint-plugin": "^5.57.1",
    "@typescript-eslint/parser": "^5.57.1",
    "@vitejs/plugin-react": "^4.0.0",
    "autoprefixer": "^10.4.14",
    "concurrently": "^8.2.2",
    "eslint": "^8.38.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.3.4",
    "fs-extra": "^11.3.2",
    "gh-pages": "^5.0.0",
    "globby": "^14.1.0",
    "gray-matter": "^4.0.3",
    "https": "^1.0.0",
    "postcss": "^8.4.23",
    "tailwindcss": "^3.3.2",
    "typescript": "^5.0.2",
    "vite": "^4.3.2",
    "vitepress": "^1.6.4",
    "vitepress-plugin-comment-with-giscus": "^1.1.15"
  }
}
